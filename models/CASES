CREATE TABLE "INTERVIEW_DB"."PLAYGROUND_JUAN_ZIMERMAN".CASES AS

SELECT
DATE,
LOCATION,
TOTAL_CASES_PER_MILLION,
TOTAL_ACTIVE_CASES,
NEW_CASES_PER_MILLION,
NEW_CASES,
NEW_ACTIVE_CASES,
TOTAL_CASES,
GROWTH_FACTOR_OF_NEW_CASES


 FROM
"FIVETRAN_INTERVIEW_DB"."GOOGLE_SHEETS"."COVID_19_INDONESIA_JUAN_ZIMERMAN"

ORDER BY DATE