CREATE TABLE "INTERVIEW_DB"."PLAYGROUND_JUAN_ZIMERMAN".DEATHS AS

SELECT 
DATE,
LOCATION,
NEW_DEATHS_PER_MILLION,
CASE_FATALITY_RATE,
NEW_DEATHS,
TOTAL_DEATHS,
TOTAL_DEATHS_PER_MILLION,
GROWTH_FACTOR_OF_NEW_DEATHS

 FROM
"FIVETRAN_INTERVIEW_DB"."GOOGLE_SHEETS"."COVID_19_INDONESIA_JUAN_ZIMERMAN"

ORDER BY DATE