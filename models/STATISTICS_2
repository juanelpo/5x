CREATE TABLE "INTERVIEW_DB"."PLAYGROUND_JUAN_ZIMERMAN".STATS2 AS

SELECT
DATE,
LOCATION,
MAX(POPULATION) AS POPULATION,
AVG(GROWTH_FACTOR_OF_NEW_DEATHS) AS AVERAGE_DEATH_GROW,
AVG(CASE_RECOVERED_RATE) AS AVERAGE_RECOVERED_RATE
FROM "FIVETRAN_INTERVIEW_DB"."GOOGLE_SHEETS"."COVID_19_INDONESIA_JUAN_ZIMERMAN"
GROUP BY LOCATION, DATE
